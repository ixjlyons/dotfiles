Xft.autohint: true
Xft.antialias: true
Xft.hinting: true
Xft.hintstyle: hintslight
Xft.dpi: 96
Xft.rgba: rgb
Xft.lcdfilter: lcddefault

! urxvt ----------------------------------------------------------------------
URxvt*transparent: true
URxvt*shading: 15
URxvt*scrollBar: false
URxvt.font: xft:DejaVuSansMono:pixelsize=11
URxvt.boldFont: xft:DejaVuSansMono:pixelsize=11:Bold
URxvt.letterSpace: -1
URxvt.iso14755: false
URxvt.intensityStyles: false

#define black       #101010
#define darkgrey    #181818

#define darkred     #962521
#define red         #b73939

#define darkgreen   #37933e
#define green       #5eb664

#define darkyellow  #cc6b1c
#define yellow      #cd7e3d

#define darkblue    #2b4e64
#define blue        #3f6883

#define darkmagenta #692d6a
#define magenta     #8c4a8e

#define darkcyan    #0f615e
#define cyan        #2e938f

#define lightgrey  #afafaf
#define white      #d5d5d5

*.background: #101010
*.foreground: #aaaaaa
*.color0:     black
*.color8:     darkgrey

*.color1: #752c1f
*.color9: #b4452f
*.color2: #867f7a
*.color10: #b4aba5
*.color3: #645c56
*.color11: #b4a79c
*.color4: #404764
*.color12: #7480b4
*.color5: #644a4a
*.color13: #b48686
*.color6: #64635f
*.color14: #b4b2ab

*.color7:     lightgrey
*.color15:    white 
