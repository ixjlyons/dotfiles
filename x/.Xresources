! font settings
Xft.antialias: true
Xft.hinting: true
Xft.hintstyle: hintmedium
Xft.dpi: 96
Xft.rgba: rgb
Xft.lcdfilter: lcddefault

! urxvt
URxvt*transparent: true
URxvt*shading: 20
URxvt*scrollBar: false
URxvt.font: xft:DejaVuSansMono:pixelsize=11
URxvt.boldFont: xft:DejaVuSansMono:pixelsize=11:Bold
URxvt.letterSpace: -1
URxvt.iso14755: false
URxvt.intensityStyles: false

*.background: #101010
*.foreground: #bbbbbb
*.cursorColor: #687496
! black
*.color0:  #282828
*.color8:  #323232
! red
*.color1:  #962d2a
*.color9:  #b43732
! green
*.color2:  #92966a
*.color10: #afb47f
! yellow
*.color3:  #966039
*.color11: #b47344
! blue
*.color4:  #687496
*.color12: #7c8bb4
! magenta
*.color5:  #765796
*.color13: #8e68b4
! cyan
*.color6:  #739396
